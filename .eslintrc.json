{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": 0,
        "eol-last": ["error", "never"],
        "indent": ["error", 4],
        "linebreak-style": ["error", "windows"],
        "semi": ["error", "always"],
        "max-len": ["error", {"code": 120}]
    }
}
